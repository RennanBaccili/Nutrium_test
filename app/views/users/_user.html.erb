<section>
  <div id="<%= dom_id user %>">
    <p>
      <strong>Full name:</strong>
      <%= user.full_name %>
    </p>

    <p>
      <strong>Email:</strong>
      <%= user.email %>
    </p>

    <p>
      <strong>Password:</strong>
      <%= user.password %>
    </p>

    <p>
      <strong>Gender:</strong>
      <%= user.gender %>
    </p>

    <p>
      <strong>Country:</strong>
      <%= user.country %>
    </p>

    <p>
      <strong>Date of birth:</strong>
      <%= user.date_of_birth %>
    </p>

    <p>
      <strong>Phone:</strong>
      <%= user.phone %>
    </p>

    <p>
      <strong>Profession:</strong>
      <%= user.profession %>
    </p>

  </div>
</section>